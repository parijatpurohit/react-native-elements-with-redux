{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "import/no-unresolved": "off"
    },
    "parser": "babel-eslint",
    "extends": "airbnb"
}